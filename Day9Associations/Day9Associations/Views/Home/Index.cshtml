@model List<Day9Associations.Models.Movie>

@{
    ViewBag.Title = "Home Page";
}


<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Title</th>
            <th>Director</th>
            <th>Category</th>   
        </tr>
    </thead>
    <tbody>
        @foreach(var movie in Model)
        {
            <tr>
                <td>@movie.Title</td>
                <td>@movie.Director</td>
                <td>@movie.Category.Name</td>
            </tr>
        }
    </tbody>
</table>